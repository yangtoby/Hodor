@{
    ViewBag.Title = "Form";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<script>
    var keyValue = $.request("keyValue");
    $(function () {
        initControl();
        $.ajax({
            url: "/CustomerManage/Customer/GetFormJson",
            data: { keyValue: keyValue },
            dataType: "json",
            async: false,
            success: function (data) {
                $("#form1").formSerialize(data);
                $("#form1").find('.form-control,select,input').attr('readonly', 'readonly');
                $("#form1").find('div.ckbox label').attr('for', '');
                //$("#F_UserPassword").val("******");
            }
        });
    });
    function initControl() {
        $("#F_Gender").bindSelect()
        $("#F_IsAdministrator").bindSelect()
        $("#F_EnabledMark").bindSelect()
        $("#F_OrganizeId").bindSelect({
            url: "/SystemManage/Organize/GetTreeSelectJson"
        });
        $("#F_Area").bindSelect({
            url: "/SystemManage/Area/GetTreeSelectJson",
        });
        $("#F_RoleId").bindSelect({
            url: "/SystemManage/Role/GetGridJson",
            id: "F_Id",
            text: "F_FullName"
        });
        $("#F_DutyId").bindSelect({
            url: "/SystemManage/Duty/GetGridJson",
            id: "F_Id",
            text: "F_FullName"
        });
        $("#F_Bank").bindSelect({
            url: '/SystemManage/ItemsData/GetGridJson',
            param: { itemId: "2748F35F-4EE2-417C-A907-3453146AAF67" },
            id: "F_Id",
            text: "F_ItemName"
        });
        $("#F_Machines").bindSelect({
            url: '/SystemManage/ItemsData/GetGridJson',
            param: { itemId: "00F76465-DBBA-484A-B75C-E81DEE9313E6" },
            id: "F_Id",
            text: "F_ItemName"
        });
    }
</script>
<form id="form1">
    <div style="margin-top: 10px; margin-left: 10px; margin-right: 10px;">
        <ul class="nav nav-tabs">
            <li class="active"><a href="#">基本信息</a></li>
            @*<li><a href="javascript:void()">扩展信息</a></li>
            <li><a href="javascript:void()">登录日志</a></li>
            <li><a href="javascript:void()">操作日志</a></li>*@
        </ul>
        <div style="padding-top: 20px; margin-right: 30px;">
            <table class="form">
                <tr>
                    <th class="formTitle">客户姓名</th>
                    <td class="formValue">
                        <input id="F_Name" name="F_Name" type="text" class="form-control required" placeholder="请输入客户姓名" />
                    </td>
                    <th class="formTitle">客户电话</th>
                    <td class="formValue">
                        <input id="F_Tel" name="F_Tel" type="text" class="form-control required" placeholder="请输入客户电话" />
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">所属银行</th>
                    <td class="formValue">
                        <select id="F_Bank" name="F_Bank" class="form-control">
                            <option value="">==请选择==</option>
                        </select>
                    </td>
                    <th class="formTitle">所属区域</th>
                    <td class="formValue">
                        <select id="F_Area" name="F_Area" class="form-control">
                            <option value="">==请选择==</option>
                        </select>
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">客户编号</th>
                    <td class="formValue">
                        <input id="F_Number" name="F_Number" type="text" class="form-control" placeholder="请输入客户编号" />
                    </td>
                    <th class="formTitle">经营范围</th>
                    <td class="formValue">
                        <input id="F_Range" name="F_Range" type="text" class="form-control" placeholder="请输入经营范围" />
                    </td>

                </tr>
                <tr>
                    <th class="formTitle">店铺名字</th>
                    <td class="formValue">
                        <input id="F_Shop_Name" name="F_Shop_Name" type="text" class="form-control" placeholder="请输入店铺名字" />
                    </td>
                    <th class="formTitle">店铺地址</th>
                    <td class="formValue">
                        <input id="F_Shop_Address" name="F_Shop_Address" type="text" class="form-control" placeholder="请输入店铺地址" />
                    </td>


                </tr>
                <tr>
                    <th class="formTitle">投放机具类型</th>
                    <td class="formValue">
                        <select id="F_Machines" name="F_Machines" class="form-control">
                            <option value="">==请选择==</option>
                        </select>
                    </td>
                    <th class="formTitle">机具编号</th>
                    <td class="formValue">
                        <input id="F_Machines_Number" name="F_Machines_Number" type="text" class="form-control" placeholder="请输入机具编号" />
                        @*<input id="F_Birthday" name="F_Birthday" type="text" class="form-control input-wdatepicker" onfocus="WdatePicker()" />*@
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">收银员名称</th>
                    <td class="formValue">
                        <input id="F_Cashier_Name" name="F_Cashier_Name" type="text" class="form-control" />
                    </td>
                    <th class="formTitle">收银员编号</th>
                    <td class="formValue">
                        <input id="F_Cashier_Number" name="F_Cashier_Number" type="text" class="form-control" />
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">客户经理名称</th>
                    <td class="formValue">
                        <input id="F_Manage_Name" name="F_Manage_Name" type="text" class="form-control" />
                    </td>
                    <th class="formTitle">客户经理电话</th>
                    <td class="formValue">
                        <input id="F_Manage_Tel" name="F_Manage_Tel" type="text" class="form-control" />
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">PSAM卡编号</th>
                    <td class="formValue">
                        <input id="F_PSAM_Number" name="F_PSAM_Number" type="text" class="form-control" />
                    </td>
                    @*<th class="formTitle"></th>
            <td class="formValue">
                <input id="F_Email" name="F_Email" type="text" class="form-control" />
            </td>*@
                </tr>
                <tr>
                    <th class="formTitle" valign="top" style="padding-top: 5px;">备注
                    </th>
                    <td class="formValue" colspan="3">
                        <textarea id="F_Remark" name="F_Remark" class="form-control" style="height: 60px;"></textarea>
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">创建人员</th>
                    <td class="formValue">
                        <input id="F_CreatorUserId" name="F_CreatorUserId" type="text" class="form-control" />
                    </td>
                    <th class="formTitle">创建时间</th>
                    <td class="formValue">
                        <input id="F_CreatorTime" name="F_CreatorTime" type="text" class="form-control" />
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">修改人员</th>
                    <td class="formValue">
                        <input id="F_LastModifyUserId" name="F_LastModifyUserId" type="text" class="form-control" />
                    </td>
                    <th class="formTitle">修改时间</th>
                    <td class="formValue">
                        <input id="F_LastModifyTime" name="F_LastModifyTime" type="text" class="form-control" />
                    </td>
                </tr>
            </table>
        </div>
    </div>
</form>
